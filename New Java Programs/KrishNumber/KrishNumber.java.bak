class KrishNumber
{
	public static void main(String[] args)
	{
		int num = 154;
		int dup = num;
		int sum = 0;
		while (num>0)
		{
			int rem = num%10;
			int fact = 1;
			for (int i = rem; i>=1; i-- )
			{
				fact *= i;
			}
				sum += fact;
				num/=10;
		}
		System.out.println(sum);
		System.out.println(dup==sum?dup+" it is Krish Number":dup+" it is not Krish Number");
	}
}